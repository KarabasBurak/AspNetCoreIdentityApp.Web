@model List<UserDto>

@{
    ViewData["Title"] = "GetUserList";
}

<h1>Member List</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Id</th>
            <th>UserName</th>
            <th>Email</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
            </tr>
        }
    </tbody>

</table>